    "less-loader": "^4.0.5",
    "lint-staged": "^5.0.0",
    "loading-cli": "^1.0.6",
    "marked": "^0.3.6",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "prismjs": "^1.8.3",
    "raw-loader": "^0.5.1",
    "react-dev-utils": "^4.1.0",
